{
  "name": "ymaps-polygonmap",
  "version": "0.0.1",
  "description": "Yandex.Maps API module for data visualization.",
  "keywords": [
    "yandex",
    "maps",
    "polygonmap"
  ],
  "main": "src/Polygonmap.js",
  "scripts": {
    "dev": "webpack-dev-server --mode development --config webpack.examples.js --open",
    "build": "npm run build-module && npm run build-examples",
    "build-module": "webpack --mode production --config webpack.module.js",
    "build-examples": "webpack --mode production --config webpack.examples.js",
    "deploy": "gh-pages -d dist",
    "publish": "np",
    "lint": "eslint src/**/*.js",
    "test": "nyc mocha -r babel-core/register src/**/*.test.js",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "docs": "jsdoc2md --template .jsdoc/README.md src/*.js > README.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "codecov": "^3.0.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-loris": "^9.1.0",
    "gh-pages": "^1.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.2",
    "jsdoc-to-markdown": "^4.0.1",
    "json-loader": "^0.5.7",
    "lint-staged": "^7.0.4",
    "mocha": "^5.1.1",
    "np": "^3.0.1",
    "nyc": "^11.8.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/yandex-shri-fx-team/ymaps-polygonmap#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yandex-shri-fx-team/ymaps-polygonmap.git"
  },
  "bugs": {
    "url": "https://github.com/yandex-shri-fx-team/ymaps-polygonmap/issues"
  },
  "dependencies": {
    "colormap": "^2.3.0",
    "robust-point-in-polygon": "^1.0.3"
  }
}
